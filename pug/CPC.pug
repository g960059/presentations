            useHTML: true,  
            itemStyle:{
              "fontSize": "30px", 
              "fontWeight": "bold"
            }
          },
          yAxis:[ {
            title: {
              text: 'CRP(mg/dl)'
            },
            labels:{
              y: 7
            },
            tickAmount: 6
            }, {
              offset:60,
              labels: {
                  enabled: false,
                  style: {
                      color: '#525151',
                      font: '10px',
                      fontWeight: 'bold',
                  }
                 /* formatter: function() {
                      if (tasks[this.value]) {
                          return tasks[this.value].name;
                      }
                  }*/
              },
              startOnTick: false,
              endOnTick: false,
              TickWidth:0,
              gridLineWidth:0,
              title: {
                  text: ''
              },
              minPadding: 0.2,
              maxPadding: 0.2
          },{ // Primary yAxis
            labels: {
                y: 7,
                formatter: function() {
                      return (this.value + 36)+"°C"
                  },
                style: {
                    color: Highcharts.getOptions().colors[3]
                }
            },
            tickAmount: 4,
            startOnTick: false,
            endOnTick: false,
            TickWidth:0,
            gridLineWidth:0,
            title: {
                text: '',
                style: {
                    color: Highcharts.getOptions().colors[3]
                }
            },
            opposite: true

        }],
          plotOptions: {
            series: {
              fillOpacity: 0,
              zoneAxis: 'x',
              lineWidth: 5,
              marker: {
                radius: 7
              }
            }
          },
          xAxis: {
            type: 'linear',
            allowDecimals: false,
            tickPixelInterval:120,
            title: {
              text: '転科後日数'
            },
            labels: {
              formatter: function() {
                return this.value+1;
              },
              y:30
            },
            plotBands: [{
              color: '#ddd', // Color value
              className: '', // Style of the plot line. Default to solid
              from: 26.5, // Value of where the line will appear
              to: 27.5,
              label:{
                text: "<div class='gray-balloon'>初回透析</div>",
                useHTML:true
              }},{
              color: '#ddd', // Color value
              className: '', // Style of the plot line. Default to solid
              from: 60.5, // Value of where the line will appear
              to: 61.5,
              label:{
                text: "<div class='gray-balloon'>動脈表在化</div>",
                useHTML:true
              }},{
              color: '#ddd', // Color value
              className: '', // Style of the plot line. Default to solid
              from: 130.5, // Value of where the line will appear
              to: 131.5,
              label:{
                text: "<div class='gray-balloon'>短期留置カテ</div>",
                useHTML:true
              }},{
              color: '#ddd', // Color value
              className: '', // Style of the plot line. Default to solid
              from: 157.5, // Value of where the line will appear
              to: 158.5,
              label:{
                text: "<div class='gray-balloon'>長期留置カテ</div>",
                useHTML:true
              }},{
              color: '#ddd', // Color value
              className: '', // Style of the plot line. Default to solid
              from: 143.5, // Value of where the line will appear
              to: 144.5,
              label:{
                text: "<div class='gray-balloon' style='top:0px;'>CT</div>",
                useHTML:true
              }},{
              color: '#c3efff', // Color value
              className: 'fg-2', // Style of the plot line. Default to solid
              from: 146.5, // Value of where the line will appear
              to: 160.5,
              label:{
                text: "<div class='balloon fg-2'style='top: 50px;left: 70px;'>胸腔ドレーン留置</div>",
                useHTML:true
              }},{
              color: '#c3efff', // Color value
              className: 'fg-2', // Style of the plot line. Default to solid
              from: 168, // Value of where the line will appear
              to: 193,
              label:{
                text: "",
                useHTML:true
              }}
            ]
          }, 
          series: data_source_.concat(gantt_source)
      });
      var counter = 0;
      var done=false;
      var series = myChart.series;
      var reset =()=> _.each(series,(data)=>{
          data.setVisible(false, false);
          data.update({ showInLegend: false}, false);
        }
      );
      var hide_chart = ()=>_.each(series,(data)=>{
        if(data.name != "task"){
          data.setVisible(false, false);
          data.update({ showInLegend: false}, false);
        }
      });
      
      var show_series=(list)=>{
        list.map((i)=>{
          series[i].show();
          series[i].update({ showInLegend: true }, false);
          }
        )
      }
      var show_series_only=(list)=>{
        hide_chart();
        list.map((i)=>{
          series[i].show();
          series[i].update({ showInLegend: true }, false);
          }
        );
        myChart.redraw();
      }
      Reveal.addEventListener( 'fragmentshown', function( event ) {
        counter++;
        render(counter);
      });
      Reveal.addEventListener( 'fragmenthidden', function( event ) {
        counter--;
        render(counter);
      });
      Reveal.addEventListener( 'slidechanged', function( event ) {
         if(!done){
          counter = 0;
         }else{
          counter=2;
         }
         render(counter);
      });
      var render = (counter) =>{
        switch(counter){
          case 0:
            show_series_only([0]);
            myChart.xAxis[0].setExtremes(-8, 145);
            done=false;
            break;
            
          case 1:
            myChart.xAxis[0].setExtremes(-8, 193);
            done=true;
            break;
            
          default: break; 
        }
      }
      render(counter);
    }); 

