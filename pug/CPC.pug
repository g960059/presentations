              from: 168, // Value of where the line will appear
              to: 193,
              label:{
                text: "",
                useHTML:true
              }}
            ]
          }, 
          series: data_source.concat(gantt_source)
      });
      var counter = 0;
      var done = false;
      var series = myChart.series;
      var reset =()=> _.each(series,(data)=>{
          data.setVisible(false, false);
          data.update({ showInLegend: false}, false);
        }
      );
      var hide_chart = ()=>_.each(series,(data)=>{
        if(data.name != "task"){
          data.setVisible(false, false);
          data.update({ showInLegend: false}, false);
        }
      });
      var hide_task = ()=>_.each(series,(data)=>{
        if(data.name == "task"){
          data.setVisible(false, false);
        }
      });
      var show_series=(list)=>{
        list.map((i)=>{
          series[i].show();
          series[i].update({ showInLegend: true }, false);
          }
        )
      }
      var show_series_only=(list)=>{
        hide_chart();
        list.map((i)=>{
          series[i].show();
          series[i].update({ showInLegend: true }, false);
          }
        );
        myChart.redraw();
      }
      var show_task_only=(list)=>{
        hide_task();
        list.map((i)=>{
          series[series.length-i-1].show();
          }
        );
        myChart.redraw();
      }
      Reveal.addEventListener( 'fragmentshown', function( event ) {
        counter++;
        render(counter);
      });
      Reveal.addEventListener( 'fragmenthidden', function( event ) {
        counter--;
        render(counter);
      });
      Reveal.addEventListener( 'slidechanged', function( event ) {
         if(!done){
          counter = 0;
         }else{
          counter=2;
         }
         render(counter);
      });
      var render = (counter) =>{
        switch(counter){
          case 0:
            show_series_only([0,1]);
            myChart.xAxis[0].setExtremes(-8, 8); 
            done=false;
            break;
          case 1:
            show_series_only([1,2,3]);
            break;
          case 2:
            myChart.xAxis[0].setExtremes(-8, 28); 
            show_series_only([0,1,4]);
            done=true;
            break;
          default: break; 
        }
      }
      render(counter);
    }); 

